{
  "openapi": "3.0.1",
  "info": {
    "title": "Lost_And_Found_Web_Portl_Api",
    "version": "v1"
  },
  "paths": {
    "/Authentication/RegisterUser": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/SecuredApiCode/RegisterAdmin": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Authentication/Login": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Authentication/Logout": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/LostAndFound/AddLostItem": {
      "post": {
        "tags": [
          "LostAndFound"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LostItemToAddDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LostItemToAddDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LostItemToAddDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/LostAndFound/GetLostItems": {
      "get": {
        "tags": [
          "LostAndFound"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/LostAndFound/GetMyLostItemsPost": {
      "get": {
        "tags": [
          "LostAndFound"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginDTO": {
        "required": [
          "password",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userName": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "LostItemToAddDTO": {
        "required": [
          "brand",
          "color",
          "date",
          "latitude",
          "longitude",
          "place",
          "type"
        ],
        "type": "object",
        "properties": {
          "type": {
            "maxLength": 120,
            "minLength": 1,
            "type": "string"
          },
          "brand": {
            "maxLength": 80,
            "minLength": 1,
            "type": "string"
          },
          "color": {
            "maxLength": 40,
            "minLength": 1,
            "type": "string"
          },
          "marks": {
            "maxLength": 500,
            "type": "string",
            "nullable": true
          },
          "place": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "latitude": {
            "type": "number",
            "format": "double"
          },
          "longitude": {
            "type": "number",
            "format": "double"
          },
          "photoBase64": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterDTO": {
        "required": [
          "confirmPassword",
          "email",
          "password",
          "personName",
          "phoneNumber"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "personName": {
            "minLength": 1,
            "type": "string"
          },
          "phoneNumber": {
            "minLength": 1,
            "pattern": "^[+0-9]*$",
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Enter 'Bearer {your JWT token}'",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}